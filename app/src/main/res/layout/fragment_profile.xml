<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/frameLayout"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".ProfileFragment">

    <ImageView
        android:id="@+id/babysitterImageView"
        android:layout_width="230dp"
        android:layout_height="230dp"
        android:layout_marginStart="141dp"
        android:layout_marginTop="20dp"
        android:layout_marginEnd="142dp"
        android:layout_marginBottom="11dp"
        android:src="@drawable/babysitterimage1"
        app:layout_constraintBottom_toTopOf="@+id/babysitterNameTextView"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/babysitterimage0" />

    <TextView
        android:id="@+id/babysitterNameTextView"
        android:layout_width="wrap_content"
        android:layout_height="30dp"
        android:layout_marginStart="100dp"
        android:layout_marginTop="5dp"
        android:layout_marginEnd="100dp"
        android:layout_marginBottom="5dp"
        android:text="Name here"
        android:textSize="22sp"
        app:layout_constraintBottom_toTopOf="@+id/babysitterRatingBar"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/babysitterImageView" />

    <RatingBar
        android:id="@+id/babysitterRatingBar"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="85dp"
        android:layout_marginEnd="86dp"
        android:clickable="false"
        android:isIndicator="true"
        android:longClickable="false"
        android:stepSize="0.1"
        app:layout_constraintBottom_toTopOf="@+id/buttonBS"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/babysitterNameTextView" />

    <Button
        android:id="@+id/buttonBS"
        android:layout_width="65dp"
        android:layout_height="0dp"
        android:layout_marginStart="55dp"
        android:layout_marginTop="6dp"
        android:height="25dp"
        android:backgroundTint="#415A70"
        android:text="BS"
        app:layout_constraintEnd_toStartOf="@+id/buttonCPR"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/babysitterRatingBar" />

    <Button
        android:id="@+id/buttonCPR"
        android:layout_width="65dp"
        android:layout_height="0dp"
        android:layout_marginTop="6dp"
        android:height="25dp"
        android:backgroundTint="#415A70"
        android:text="Cpr"
        app:iconTint="#335D83"
        app:layout_constraintEnd_toStartOf="@+id/buttonCAR"
        app:layout_constraintStart_toEndOf="@+id/buttonBS"
        app:layout_constraintTop_toBottomOf="@+id/babysitterRatingBar" />

    <Button
        android:id="@+id/buttonCAR"
        android:layout_width="65dp"
        android:layout_height="0dp"
        android:layout_marginTop="6dp"
        android:height="25dp"
        android:backgroundTint="#415A70"
        android:text="car"
        app:layout_constraintEnd_toStartOf="@+id/button5Y"
        app:layout_constraintStart_toEndOf="@+id/buttonCPR"
        app:layout_constraintTop_toBottomOf="@+id/babysitterRatingBar" />

    <Button
        android:id="@+id/button5Y"
        android:layout_width="75dp"
        android:layout_height="0dp"
        android:layout_marginTop="6dp"
        android:layout_marginEnd="55dp"
        android:height="25dp"
        android:backgroundTint="#415A70"
        android:paddingLeft="5dp"
        android:paddingRight="5dp"
        android:text="5y exp"
        android:textAllCaps="false"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/buttonCAR"
        app:layout_constraintTop_toBottomOf="@+id/babysitterRatingBar" />

    <Button
        android:id="@+id/viewProfileButton"
        android:layout_width="151dp"
        android:layout_height="60dp"
        android:layout_marginStart="137dp"
        android:layout_marginTop="40dp"
        android:layout_marginEnd="136dp"
        android:backgroundTint="#415A70"
        android:text="View Profile"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/buttonCPR" />

    <Button
        android:id="@+id/messageButton"
        android:layout_width="150dp"
        android:layout_height="60dp"
        android:layout_marginStart="130dp"
        android:layout_marginTop="30dp"
        android:layout_marginEnd="131dp"
        android:layout_marginBottom="110dp"
        android:backgroundTint="#415A70"
        android:text="Message"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/viewProfileButton" />

</androidx.constraintlayout.widget.ConstraintLayout>